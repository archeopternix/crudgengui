{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Go Program",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/crudgengui/main.go",
            "env": {},
            "args": [],
            "showLog": true
        },
        {
            "name": "Launch Go Tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [],
            "showLog": true
        },
        {
            "name": "Build Go Project",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/crudgengui",
            "env": {},
            "args": ["build"],
            "showLog": true,
            "preLaunchTask": "go build"
        },
        {
            "name": "Lint Go Project",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": ["vet", "./..."],
            "showLog": true,
            "preLaunchTask": "go vet"
        }
    ]
}
